{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 2],
        "quotemark": [true,"single"],
        "no-console": [true, "time", "timeEnd", "trace"],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "max-line-length": false
    },
    "rulesDirectory": []
}